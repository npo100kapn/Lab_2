function reduce(array, fn, initial) {
	let i = 0;
    	let result = initial || array[i++];
    	while (i < array.length) {
        	result = fn(result, array[i], i, array);
        	i++;
    	}
    	console.log (result); 
}

let arr = [3, 5, 6, 2];
function funcx (sum, currentEntry) {
        return sum + currentEntry;
}
    
reduce(arr, funcx, 2);